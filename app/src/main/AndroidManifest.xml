<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aramirez.trabajo_grupal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Examen_Ramirez">

        <activity
            android:name=".MainLogin"
            android:exported="true"
            android:label="LOGIN APP">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Registrar"
            android:exported="true"
            android:label="REGISTRO de USUARIOS"
            android:parentActivityName=".MainLogin">
        </activity>

        <activity
            android:name=".Principal"
            android:exported="true"
            android:label="Bienvenidos"
            android:parentActivityName=".MainLogin">

        </activity>
        <activity
            android:name=".InicioActivity"
            android:exported="true"
            android:label="Personal a Ingresar - ABC SAC"
            android:parentActivityName=".Principal" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="Registro de personal"
            android:parentActivityName=".InicioActivity" />
        <activity
            android:name=".inicio_app"
            android:exported="true"
            android:label="Aplicaciones"
            android:parentActivityName=".Principal" />
        <activity
            android:name=".aplicaciones"
            android:exported="true"
            android:label="Registro de Aplicaciones"
            android:parentActivityName=".inicio_app" />
        <activity
            android:name=".inicio_sedes"
            android:exported="true"
            android:label="Lista de Sedes"
            android:parentActivityName=".Principal" />
        <activity
            android:name=".main_sedes"
            android:exported="true"
            android:label="Registro de Sedes"
            android:parentActivityName=".inicio_sedes" />
    </application>

</manifest>