<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:padding="25dp"
tools:context=".aplicaciones">

<TextView
    android:id="@+id/txtNantenimiento"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="12dp"
    android:text="Mantenimiento de Aplicaciones"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:ignore="MissingConstraints" />


<EditText
    android:id="@+id/txtNombre"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="25dp"
    android:background="@drawable/caja_personalizada"
    android:drawableStart="@drawable/app"
    android:drawablePadding="12dp"
    android:ems="10"
    android:hint="Ingrese nombre de la aplicación"
    android:inputType="textPersonName"
    android:minHeight="48dp"
    android:paddingHorizontal="12dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtNantenimiento" />

<EditText
    android:id="@+id/txtUrl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="25dp"
    android:background="@drawable/caja_personalizada"
    android:drawableStart="@drawable/ic_url"
    android:drawablePadding="12dp"
    android:ems="10"
    android:hint="Ingrese URL"
    android:inputType="textPersonName"
    android:minHeight="48dp"
    android:paddingHorizontal="12dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtNombre" />

<EditText
    android:id="@+id/txtCategoria"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="25dp"
    android:background="@drawable/caja_personalizada"
    android:drawableStart="@drawable/ic_categoria"
    android:drawablePadding="12dp"
    android:ems="10"
    android:hint="Ingrese Categoría"
    android:inputType="textPersonName"
    android:minHeight="48dp"
    android:paddingHorizontal="12dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtUrl" />

<EditText
    android:id="@+id/txtCantidad"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="25dp"
    android:background="@drawable/caja_personalizada"
    android:drawableStart="@drawable/ic_cantidad"
    android:drawablePadding="12dp"
    android:ems="10"
    android:hint="Ingrese Cantidad"
    android:inputType="number"
    android:minHeight="48dp"
    android:paddingHorizontal="12dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtCategoria"
    tools:ignore="SpeakableTextPresentCheck" />

<EditText
    android:id="@+id/txtCosto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="25dp"
    android:background="@drawable/caja_personalizada"
    android:drawableStart="@drawable/ic_costo"
    android:drawablePadding="12dp"
    android:ems="10"
    android:hint="Ingrese costo por usuario"
    android:inputType="numberDecimal"
    android:minHeight="48dp"
    android:paddingHorizontal="12dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtCantidad" />

<EditText
    android:id="@+id/txtEquipoCargo"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="25dp"
    android:background="@drawable/caja_personalizada"
    android:drawableStart="@drawable/ic_team"
    android:drawablePadding="12dp"
    android:ems="10"
    android:hint="Ingresar Equipo a Cargo"
    android:inputType="textPersonName"
    android:minHeight="48dp"
    android:paddingHorizontal="12dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtCosto" />


<Button
    android:id="@+id/btnRegistrar"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="25dp"
    android:text="Registrar"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtEquipoCargo" />




</androidx.constraintlayout.widget.ConstraintLayout>